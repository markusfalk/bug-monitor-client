{
  "name": "bug-monitor-client",
  "version": "1.2.0",
  "description": "Client side script for tracking JS errors",
  "main": "dist/bug-monitor.client.js",
  "types": "dist/typings.d.ts",
  "scripts": {
    "test": "eslint src/bug-monitor-client",
    "uglify": "uglifyjs --compress --mangle --screw-ie8 --source-map --comments --stats --wrap-iife --quotes=3 --output ./dist/bug-monitor.client.js ./src/bug-monitor-client.js --verbose",
    "types": "cp ./src/typings.d.ts ./dist/",
    "build": "npm run uglify && npm run types",
    "serve": "serve .",
    "watch": "node_modules/onchange/cli.js './src/*.*' -- npm run build",
    "preversion": "npm run test",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markusfalk/bug-monitor-client.git"
  },
  "author": "mail@markus.falk.com",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/markusfalk/bug-monitor-client/issues"
  },
  "homepage": "https://github.com/markusfalk/bug-monitor-client#readme",
  "devDependencies": {
    "eslint": "^3.19.0",
    "install": "^0.10.1",
    "npm": "^4.6.1",
    "onchange": "^3.2.1",
    "serve": "^5.1.5",
    "uglify-js": "^3.0.11"
  },
  "dependencies": {}
}
